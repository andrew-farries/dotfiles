{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "CLAUDE_CODE_DISABLE_FEEDBACK_SURVEY": "1"
  },
  "permissions": {
    "allow": [
      "WebFetch",
      "WebSearch",
      "Bash(find:*)",
      "Bash(git log:*)",
      "Bash(git show:*)",
      "Bash(rg:*)"
    ]
  },
  "alwaysThinkingEnabled": false
}
